{
    "manifest_version": 3,
    "name": "Tab Manager Extension",
    "version": "1.0",
    "description": "A smart tool to manage and organize browser tabs",
    "permissions": ["tabs", "storage", "activeTab"],
    "action": {
      "default_popup": "popup/popup.html"
    },
    "background": {
      "service_worker": "background/background.js"
    }
  }
  